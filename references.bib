	urldate = {2024-11-04},
	journal = {The Lancet Infectious Diseases},
	author = {Lynskey, Nicola N and Jauneikaite, Elita and Li, Ho Kwong and Zhi, Xiangyun and Turner, Claire E and Mosavie, Mia and Pearson, Max and Asai, Masanori and Lobkowicz, Ludmila and Chow, J Yimmy and Parkhill, Julian and Lamagni, Theresa and Chalker, Victoria J and Sriskandan, Shiranee},
	month = nov,
