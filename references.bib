	keywords = {Algorithms, Bacteria, Archaea, Binding Sites, Computational Biology, Genes, Bacterial, Molecular Sequence Annotation, Operon, Prokaryotic Cells, Protein Biosynthesis, Proteomics, Ribosomes, Transcription, Genetic},
	pages = {1079--1089},
