	keywords = {Bacterial genomics, Genomics, Phylogenetic analysis, Phylogenetics, Legionella pneumophila, Pseudomonas aeruginosa, Sequence alignment, Single nucleotide polymorphisms},
	pages = {e1008678},
	file = {Full Text PDF:/home/rmamede/Zotero/storage/MLEDN8GE/Valiente-Mullor et al. - 2021 - One is not enough On the effects of reference gen.pdf:application/pdf},
